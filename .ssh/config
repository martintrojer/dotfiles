# do 'ssh-add ~/.ssh/id_rsa' once to register key

Host bubba
  SendEnv LANG LC_*
  LocalForward 6667 localhost:6667
  ForwardAgent yes

Host martintrojer.no-ip.biz
  SendEnv LANG LC_*
  LocalForward 6667 localhost:6667
  ForwardAgent yes

## Local Devbox
Host localhost
  SendEnv LANG LC_*
  ForwardAgent yes

## AM AWS
Host 10.0.0.*
  SendEnv LANG LC_*
  ForwardAgent yes
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
