# do 'ssh-add ~/.ssh/id_rsa' once to register key

Host bubba
  SendEnv LANG LC_*
  User aphex
  ForwardAgent yes

## Local Devbox
Host localhost
  SendEnv LANG LC_*
  ForwardAgent yes
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

## AM AWS
Host 10.0.0.*
  SendEnv LANG LC_*
  ForwardAgent yes
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
